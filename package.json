		"format:check": "prettier --check ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.15.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.7.3",
		"vite": "^6.0.7"
	},
	"type": "module",
	"engines": {
		"node": ">=20"
	}
}
